{
  "manifest_version": 2,
  "name": "JaSON",
  "version": "0.0",
  "description": "JaSON: JSON and XML test utility.",
  "app": {
    "launch": {
	  "local_path": "JaSON.html"    
    }
  },
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}